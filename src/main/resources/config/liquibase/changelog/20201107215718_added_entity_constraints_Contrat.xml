<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Contrat.
    -->
    <changeSet id="20201107215718-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="bien_id"
                                 baseTableName="contrat"
                                 constraintName="fk_contrat_bien_id"
                                 referencedColumnNames="id"
                                 referencedTableName="biens"/>

        <addForeignKeyConstraint baseColumnNames="locataire_id"
                                 baseTableName="contrat"
                                 constraintName="fk_contrat_locataire_id"
                                 referencedColumnNames="id"
                                 referencedTableName="locataire"/>

    </changeSet>
</databaseChangeLog>
